# Database connection
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db/trailblaze

# API settings
DEBUG=false
LOG_LEVEL=INFO

# CORS settings
CORS_ORIGINS=http://localhost,http://localhost:3000,http://localhost:8000

# Gemini AI settings
GEMINI_API_KEY=your_gemini_api_key
