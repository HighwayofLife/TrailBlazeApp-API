# Database connection
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db/trailblaze

# API settings
DEBUG=false
LOG_LEVEL=INFO

# CORS settings
CORS_ORIGINS=http://localhost,http://localhost:3000,http://localhost:8000

# Gemini AI settings
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.0-flash-lite

# Scraping settings
SCRAPING_SCHEDULE=0 0 * * *  # Daily at midnight

# JWT
SECRET_KEY=your_secret_key

# Geocoding settings
GEOCODING_PROVIDER=nominatim
GEOCODING_USER_AGENT=TrailBlazeApp
GEOCODING_API_KEY=
GEOCODING_TIMEOUT=5
GEOCODING_RETRIES=3
